{
    "name": "capevace/cabinet-ui",
    "description": "The UI package for Cabinet, the streamlined file management solution for Laravel.",
    "minimum-stability": "stable",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Cabinet\\Filament\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cabinet\\Filament\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Lukas Mateffy",
            "email": "hello@mateffy.me"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.92",
        "illuminate/contracts": "^12.0 || ^11.0 || ^10.0",
        "filament/forms": "^4",
        "filament/infolists": "^4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cabinet\\Filament\\CabinetFilamentServiceProvider"
            ]
        }
    },
    "require-dev": {
        "filament/upgrade": "^4.0"
    }
}
